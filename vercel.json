{
    "version": 2,
    "builds": [
        {
            "src": "server/index.ts",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/public/**"
                ]
            }
        },
        {
            "src": "dist/public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "/public/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ],
    "env": {
        "GOOGLE_API_KEY": "AIzaSyDEPEgUlqSxhWtZ30lBoQYKIMX8U0fwZlA"
    }
}